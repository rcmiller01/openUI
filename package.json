{
  "name": "open-deep-coder",
  "version": "0.1.0",
  "description": "An agentic IDE with multi-agent coding workflow powered by LLM integration",
  "main": "dist/main.js",
  "scripts": {
    "dev": "cd frontend && vite",
    "build": "cd frontend && vite build",
    "preview": "cd frontend && vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build"
  },
  "keywords": [
    "agentic-ide",
    "ai-coding",
    "multi-agent",
    "llm-integration",
    "development-tools",
    "openrouter",
    "ollama",
    "lsp",
    "mcp"
  ],
  "author": "rcmiller01",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rcmiller01/openUI.git"
  },
  "bugs": {
    "url": "https://github.com/rcmiller01/openUI/issues"
  },
  "homepage": "https://github.com/rcmiller01/openUI#readme",
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.0",
    "@codemirror/lang-json": "^6.0.0",
    "@codemirror/lang-python": "^6.1.0",
    "@codemirror/state": "^6.4.0",
    "@codemirror/view": "^6.23.0",
    "@monaco-editor/react": "^4.6.0",
    "@tauri-apps/api": "^1.5.0",
    "axios": "^1.6.0",
    "immer": "^10.0.0",
    "json-schema": "^0.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "styled-components": "^6.1.0",
    "ws": "^8.14.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.5.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/ui": "^0.34.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "jest-environment-jsdom": "^30.1.1",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.0",
    "vitest": "^0.34.0"
  }
}
